meta {
  name: 06-BanUser-Admin
  type: http
  seq: 6
}

delete {
  url: {{baseUrl}}/api/users/{{targetUserId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{adminToken}}
}

script:pre-request {
  if (!bru.getEnvVar("targetUserId")) {
    bru.setEnvVar("targetUserId", "99999999");
  }
}

tests {
  test("DELETE (ban) usuario como admin devuelve 204 o 404", function() {
    expect([204, 404]).to.include(res.status);
  });
}
