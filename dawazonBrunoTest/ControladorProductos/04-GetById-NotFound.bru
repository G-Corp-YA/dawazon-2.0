tests {
  test("GET producto inexistente devuelve 404", function() {
    expect(res.status).to.equal(404);
  });
  test("Respuesta contiene mensaje de error", function() {
    if (res.body && typeof res.body === "object") {
      expect(res.body).to.have.property("message");
    }
  });
}
