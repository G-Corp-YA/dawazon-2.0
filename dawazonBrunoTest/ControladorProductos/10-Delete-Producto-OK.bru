tests {
  test("DELETE producto con token de admin devuelve 200", function() {
    expect(res.status).to.equal(200);
  });
  test("Respuesta contiene el producto eliminado", function() {
    expect(res.body).to.have.property("id");
    expect(res.body.id).to.equal(bru.getVar("newProductId"));
  });
}
