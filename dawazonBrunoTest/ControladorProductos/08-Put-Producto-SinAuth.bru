put {
  url: {{baseUrl}}/api/products/{{newProductId}}
  body: multipartForm
  auth: none
}

body:multipart-form {
}

script:pre-request {
  if (!bru.getEnvVar("newProductId")) {
    bru.setEnvVar("newProductId", "1");
  }
}

tests {
  test("PUT sin autenticacion devuelve 401", function() {
    expect(res.status).to.equal(401);
  });
}
